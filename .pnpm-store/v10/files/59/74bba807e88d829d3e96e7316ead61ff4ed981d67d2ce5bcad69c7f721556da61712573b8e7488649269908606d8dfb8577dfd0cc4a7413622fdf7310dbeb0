/**
* Get diagnostic information for debugging
*/
function getDiagnosticInfo() {
	return {
		nodeVersion: node_process.version,
		KubbVersion: version,
		platform: process.platform,
		arch: process.arch,
		cwd: process.cwd()
	};
}

