 the current language set by the parent `Editor` component.\n * @deprecated use `useParser` instead\n */\nexport function useEditor(): ParserContextProps {\n  return useContext(Parser.Context)\n}\n\nexport function useParser(): ParserContextProps {\n  return useContext(Parser.Context)\n}\n"],"mappings":";;;;;;;;;;;;AAAA;;;ACAA;AAAA,mBAA2B;AAUpB,SAAS,UAA8E;AAC5F,QAAM,WAAO,yBAAW,KAAK,OAAO;AAEpC,SAAO;AACT;;;ACdA;AAEA,IAAAA,gBAA2B;AAcpB,SAAS,SAA4F;AAC1G,QAAM,UAAM,0BAAW,IAAI,OAAO;AAElC,MAAI,CAAC,KAAK;AACR,UAAM,IAAI,MAAM,sBAAsB;AAAA,EACxC;AAEA,SAAO;AAAA,IACL,QAAQ,IAAI;AAAA,IACZ,eAAe,IAAI;AAAA,IACnB,aAAa,IAAI,cAAc;AAAA,IAC/B,SAAS,IAAI,cAAc,QAAQ,KAAK,IAAI,aAAa;AAAA,IACzD,MAAM,IAAI;AAAA,EACZ;AACF;;;AC9BA;AAAA,IAAAC,gBAA2B;AAUpB,SAAS,YAAgC;AAC9C,aAAO,0BAAW,OAAO,OAAO;AAClC;AAEO,SAAS,YAAgC;AAC9C,aAAO,0BAAW,OAAO,OAAO;AAClC;","names":["import_react","import_react"]}